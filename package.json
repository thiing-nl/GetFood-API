    "mocha-junit-reporter": "^1.18.0",
    "mongoose": "5.7.5",
    "mongoose-bcrypt": "^1.6.0",
    "mongoose-unique-validator": "^2.0.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "qrcode": "^1.3.3",
    "serve-static": "^1.13.1",
